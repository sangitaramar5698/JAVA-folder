class Solution {
    public int search(int[] nums, int target) {
        int count =0, pos=0;
        for(int i =0;i<nums.length;i++){
            if(nums[i] == target){
                count ++;
                pos = i;
                
            }
        }if(count ==0){
            return -1;
        }else{
                return pos;
        }
        
    }
}
